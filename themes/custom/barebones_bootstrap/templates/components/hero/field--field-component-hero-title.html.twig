{#
/**
 * @file
 * Hero title field override.
 *
 */
#}
{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
    'display-4',
  ]
%}
{% for item in items %}
  <h1{{ attributes.addClass(classes) }}>{{ item.content }}</h1>
{% endfor %}
