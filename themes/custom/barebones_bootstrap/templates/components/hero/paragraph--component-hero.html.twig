{#
/**
 * @file
 * Default theme implementation to display a paragraph.
 *
 */
#}
{%
  set classes = [
    'jumbotron',
    'jumbotron-fluid',
    'full-width',
  ]
%}
{% block paragraph %}
  <div{{ attributes.addClass(classes) }}>
    {% block content %}
      {{ content }}
    {% endblock %}
  </div>
{% endblock paragraph %}
